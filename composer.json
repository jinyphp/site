{
    "name": "jiny/site",
    "description": "jinySite for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "jiny/laravel": "^0.8",
        "jiny/theme": "^0.8",
        
        "jiny/site-menu": "^0.6",
        "jiny/site-cms": "^0.4",
        "jiny/site-board": "^0.5",
        "jiny/site-page": "^0.4"

    },
    "autoload": {
        "psr-4": {
            "Jiny\\Site\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php",
            "src/Helpers/Info.php",
            "src/Helpers/View.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Site\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Site\\JinySiteServiceProvider"
            ]
        }
    }

}
